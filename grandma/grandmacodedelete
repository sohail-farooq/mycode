
#!/usr/bin/env python3


# [For every [toy] I find in the [hallway] I'm going to [throw them away]!

hallway = ["toy","lego","fork","cheetos"]

approved_hallway_item = ["toy", "lego"]

for x in hallway:
	print("\nFor every " + x, end= " I find in the hallway I'm going to throw them away!")
	if x not in approved_hallway_item:
		print(" - it's okay", end="")

print("\n loop ended")                      
	



